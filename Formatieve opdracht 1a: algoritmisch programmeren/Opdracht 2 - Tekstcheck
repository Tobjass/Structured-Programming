string1 = input("Geef een string: ")
string2 = input("Geef een string: ")

index = 0
while True:
    if string1[index] != string2[index]:
        break
    else:
        index += 1
print("Het eerste verschil zit op index: {}\n\nString1: {}{}{}{}{}\nString2: {}{}{}{}{}"
      .format(index, string1[index],string1[index+1],string1[index+2],string1[index+3],string1[index+4],
      string2[index],string2[index+1],string2[index+2],string2[index+3],string2[index+4]))
